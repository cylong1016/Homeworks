<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="user" namespace="/user" extends="struts-default">
		<action name="login" class="edu.nju.tss.action.LoginAction">
			<result name="success" type="redirectAction">courselist</result>
			<result name="error">/user/login.jsp</result>
		</action>
		<action name="courselist" class="edu.nju.tss.action.CourseListAction">
			<result name="success">/courselist</result>
		</action>
	</package>
</struts>
